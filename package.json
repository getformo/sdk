{
  "name": "@formo/analytics",
  "version": "1.20.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getformo/sdk.git"
  },
  "main": "dist/cjs/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "module": "dist/esm/src/index.js",
  "unpkg": "dist/index.umd.min.js",
  "files": [
    "dist/cjs/src",
    "dist/esm/src",
    "dist/index.umd.min.js",
    "dist/index.umd.min.js.LICENSE.txt"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "ethereum-cryptography": "3.2.0",
    "fetch-retry": "6.0.0",
    "mipd": "0.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.x",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.3",
    "@semantic-release/npm": "12.0.1",
    "@size-limit/esbuild": "^11.2.0",
    "@size-limit/file": "^11.2.0",
    "@testing-library/react": "^13.4.0",
    "@types/chai": "^4.3.1",
    "@types/jsdom": "^20.0.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/sinon": "^10.0.12",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "chai": "^4.3.6",
    "commitizen": "^4.2.5",
    "conventional-changelog-conventionalcommits": "9.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-react-app": "^7.0.1",
    "genversion": "^3.2.0",
    "global-jsdom": "^8.6.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-release": "24.2.5",
    "semantic-release-export-data": "^1.0.1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "size-limit": "^11.2.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "typescript": "^5.7.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build-cjs && yarn build-esm && yarn webpack --mode=production && yarn clean-build",
    "build-cjs": "yarn tsc --build",
    "build-esm": "yarn tsc -m es6 --outdir dist/esm",
    "clean": "rm -rf dist",
    "clean-build": "find dist -name '*.map' -delete && find dist -name '*.tsbuildinfo' -delete && find dist -name 'test' -type d -exec rm -rf {} \\;",
    "lint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "test": "mocha --require ts-node/register --project tsconfig.test.json test/**/*.ts",
    "test-watch": "nodemon --config test.nodemon.json",
    "prepare": "husky install",
    "commit": "git add . && cz",
    "pub": "yarn build && npm publish",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.umd.min.js",
      "limit": "32 KB"
    }
  ],
  "peerDependencies": {
    "@types/react": ">=16.14.34",
    "react": ">=16.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
