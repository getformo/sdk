{
  "name": "@formo/analytics",
  "version": "1.16.20",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getformo/sdk.git"
  },
  "main": "dist/cjs/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "module": "dist/esm/src/index.js",
  "unpkg": "dist/index.umd.min.js",
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "ethereum-cryptography": "3.2.0",
    "fetch-retry": "6.0.0",
    "mipd": "0.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/plugin-syntax-flow": "7.27.1",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.3",
    "@semantic-release/npm": "12.0.1",
    "@size-limit/esbuild": "11.2.0",
    "@size-limit/file": "11.2.0",
    "@testing-library/react": "16.3.0",
    "@types/chai": "5.2.2",
    "@types/jsdom": "21.1.7",
    "@types/mocha": "10.0.10",
    "@types/node": "24.0.0",
    "@types/react": "19.1.7",
    "@types/sinon": "17.0.4",
    "@types/sinon-chai": "4.0.0",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "chai": "5.2.0",
    "commitizen": "4.3.1",
    "conventional-changelog-conventionalcommits": "9.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.28.0",
    "eslint-config-react-app": "7.0.1",
    "genversion": "3.2.0",
    "global-jsdom": "^8.6.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "mocha": "11.6.0",
    "nodemon": "3.1.10",
    "nyc": "17.1.0",
    "prettier": "^2.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-release": "24.2.5",
    "semantic-release-export-data": "1.1.0",
    "sinon": "20.0.0",
    "sinon-chai": "^3.7.0",
    "size-limit": "11.2.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "webpack": "5.99.9",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn test && yarn build-cjs && yarn build-esm && yarn webpack --mode=production",
    "build-cjs": "yarn tsc --build",
    "build-esm": "yarn tsc -m es6 --outdir dist/esm",
    "clean": "rm -rf dist",
    "lint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test-watch": "nodemon --config test.nodemon.json",
    "prepare": "husky install",
    "commit": "git add . && cz",
    "pub": "yarn build && npm publish",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.umd.min.js",
      "limit": "27 KB"
    }
  ],
  "peerDependencies": {
    "@types/react": ">=16.14.34",
    "react": ">=16.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
